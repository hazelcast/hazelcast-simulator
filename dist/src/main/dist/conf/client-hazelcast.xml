<?xml version="1.0" encoding="UTF-8"?>

<hazelcast-client
        xsi:schemaLocation="http://www.hazelcast.com/schema/client-config
            http://www.hazelcast.com/schema/config/hazelcast-client-config-3.2.xsd"
        xmlns="http://www.hazelcast.com/schema/client-config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <group>
        <name>workers</name>
    </group>

    <network>
        <cluster-members>
            <!--MEMBERS-->
        </cluster-members>
    </network>

    <serialization>
        <portable-version>1</portable-version>
        <portable-factories>
            <portable-factory factory-id="10000000">
                com.hazelcast.stabilizer.tests.backpressure.SyntheticRequestPortableFactory
            </portable-factory>
            <portable-factory factory-id="300000">
                com.hazelcast.stabilizer.tests.map.PredicatesTest$PortableEmployeeFactory
            </portable-factory>
        </portable-factories>
        <data-serializable-factories>
            <data-serializable-factory factory-id="3000">
                com.hazelcast.stabilizer.tests.map.PredicatesTest$EmployeeDataSerializableFactory
            </data-serializable-factory>
        </data-serializable-factories>
    </serialization>

</hazelcast-client>